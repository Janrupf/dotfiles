URxvt.termName: rxvt-unicode
URxvt.loginShell: false
URxvt.font: xft:Hack Nerd Font:pixelsize=12:minspace=True
URxvt.letterSpace: 0

!URXvt.tintColor

! special
*.foreground:   #aeaeae
*.background:   #1e2127
*.cursorColor:  #aeaeae

! black
*.color0:       #333946
*.color8:       #586887

! red
*.color1:       #bf616a
*.color9:       #bf616a

! green
*.color2:       #9bd56a
*.color10:      #a3be8c

! yellow
*.color3:       #e5b75c
*.color11:      #dcbd7e

! blue
*.color4:       #6a97c3
*.color12:      #4e7195

! magenta
*.color5:       #b48ead
*.color13:      #b48ead

! cyan
*.color6:       #88c0d0
*.color14:      #1793d1

! white
*.color7:       #e5e9f0
*.color15:      #c5c8c6

! URxvt*depth: 32

*antialias: true
*autohint: true

URxvt*transparent: false
URxvt*scrollBar: false
URxvt*scrollBar_right: true
URxvt.internalBorder: 15
URxvt.url-launcher: firefox

